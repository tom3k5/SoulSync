{
  "name": "soulsync",
  "version": "1.0.0",
  "main": "index.js",
  "description": "A mindfulness and meditation mobile application",
  "author": "SoulSync Team",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clear": "expo start --clear",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security:audit": "ts-node -P scripts/tsconfig.json scripts/security-audit.ts",
    "security:fix": "ts-node -P scripts/tsconfig.json scripts/security-auto-fix.ts",
    "security:fix:dry": "ts-node -P scripts/tsconfig.json scripts/security-auto-fix.ts --dry-run",
    "security:setup": "bash scripts/setup-security-hooks.sh",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@lottiefiles/dotlottie-react": "^0.17.5",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.9",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "@types/react": "~19.1.10",
    "expo": "54.0.13",
    "expo-av": "^16.0.7",
    "expo-calendar": "^15.0.7",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-notifications": "^0.32.12",
    "expo-print": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-speech": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "lottie-react-native": "^7.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "^0.82.0",
    "react-native-pdf": "^6.7.7",
    "react-native-purchases": "^9.5.4",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.17.1",
    "react-native-web": "^0.21.2",
    "react-native-worklets-core": "^1.6.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-secrets": "^1.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "react-native-worklets": "^0.6.1",
    "react-test-renderer": "^19.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  }
}
